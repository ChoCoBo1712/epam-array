{ "configuration": {
    "properties": {
      "property": {
        "name": "pattern",
        "value": "%d %5level %logger{5} - %msg%n"
      }
    },
    "appenders": {
      "Console": {
        "name": "Console",
        "target": "SYSTEM_OUT",
        "PatternLayout": {
          "pattern": "${pattern}"
        }
      }
    },
    "loggers": {
      "root": {
        "level": "info",
        "AppenderRef": {
          "ref": "Console"
        }
      }
    }
  }
}